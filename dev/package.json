{
  "name": "rss-reader",
  "version": "4.0.0-beta.1",
  "description": "Desktop rss-reader for RSS/Atom/RDF",
  "main": "src/index.js",
  "scripts": {
    "start": "electron src/index.js",
    "test": "jest --watchAll",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "cca": "create-component-app --config ccarc.config.json",
    "build": "webpack --mode production --progress",
    "build:dev": "webpack --mode development --watch --progress",
    "pack": "npm run build && electron-builder -mw",
    "pack:mac": "npm run build && electron-builder --mac",
    "pack:win": "npm run build && electron-builder --win"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zonbitamago/rss-reader.git"
  },
  "bugs": {
    "url": "https://github.com/zonbitamago/rss-reader/issues"
  },
  "build": {
    "productName": "rss-reader",
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "pack"
    }
  },
  "homepage": "https://github.com/zonbitamago/rss-reader#readme",
  "keywords": [],
  "author": "zonbitamago",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.4.8",
    "@storybook/react": "^4.0.0-alpha.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^2.0.4",
    "electron-builder": "^20.19.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest-cli": "^23.3.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-mock-axios": "^2.1.11",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "mockdate": "^2.0.2",
    "rc-util": "^4.5.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "dotenv": "^6.0.0",
    "electron-load-devtool": "^1.0.0",
    "feedparser": "^2.2.9",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-icons": "^2.2.7",
    "string-to-stream": "^1.1.1",
    "twitter": "^1.7.1"
  }
}