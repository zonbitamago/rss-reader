    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "electron": "^1.7.5",
    "electron-builder": "^19.22.1",
    "electron-packager": "^8.7.2",
    "jest-cli": "^20.0.4",
    "react-addons-test-utils": "^15.6.0",
    "spectron": "^3.7.2"
