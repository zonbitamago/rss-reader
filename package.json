    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-register": "^6.24.1",
    "feedme": "^1.0.1",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.5",
    "react-loading-animation": "^1.3.0",
    "react-tooltip": "^3.3.0",
    "semantic-ui-css": "^2.2.12",
